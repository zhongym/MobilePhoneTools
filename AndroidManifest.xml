<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhong.mobilephonetools"
    android:versionCode="2"
    android:versionName="2.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".StartupActivity"
            android:label="@string/app_name"
           >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.zhong.mobilephonetools.MainActivity" android:launchMode="singleTask"/>
        <activity android:name="com.zhong.mobilephonetools.BottomItemActivity"/>
        <activity android:name="com.zhong.mobilephonetools.SettingActivity" android:launchMode="singleTask"/>
        <activity android:name="com.zhong.mobilephonetools.LostFindActivity" android:launchMode="singleTask"/>
        <activity android:name="com.zhong.mobilephonetools.Steup1Activity"/>
        <activity android:name="com.zhong.mobilephonetools.Steup2Activity"/>
        <activity android:name="com.zhong.mobilephonetools.Steup3Activity"/>
        <activity android:name="com.zhong.mobilephonetools.Steup4Activity"/>
        
    </application>

</manifest>